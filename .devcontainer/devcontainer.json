{
    "name": "Project Development",
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "features": {
        "ghcr.io/devcontainers/features/postgresql:1": {
            "version": "latest",
            "pgAdmin": false,
            "installPostgis": false
